{
  "name": "@repo/client",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "check-types": "tsc --noEmit",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "@repo/lib": "*",
    "@repo/schemas": "*",
    "next": "15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-redux": "^9.2.0",
    "uuid": "^11.0.4",
    "zod": "3.25.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@repo/config-tailwind": "*",
    "@repo/config-typescript": "*",
    "@repo/test": "*",
    "concurrently": "^9.1.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "exports": {
    "./*": "./src/*",
    "./styles.css": "./dist/index.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": ["**/*.css"]
}
