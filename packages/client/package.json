{
  "name": "@repo/client",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias && tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "check-types": "tsc --noEmit",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "@repo/lib": "*",
    "@repo/schemas": "*",
    "next": "15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-redux": "^9.2.0",
    "uuid": "^11.0.4",
    "zod": "^3.24.1",
    "zod-formik-adapter": "^1.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@repo/config-tailwind": "*",
    "@repo/config-typescript": "*",
    "@repo/test": "*",
    "concurrently": "^9.1.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsc-alias": "^1.8.10",
    "tsc-watch": "^6.2.1",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "exports": {
    "./*": "./dist/*.js",
    "./styles.css": "./dist/index.css"
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": ["dist/*.d.ts"]
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": ["./*.ts"]
    }
  },
  "sideEffects": ["**/*.css"]
}
