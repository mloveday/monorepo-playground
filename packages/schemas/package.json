{
  "name": "@repo/schemas",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "check-types": "tsc --noEmit",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@repo/config-typescript": "*",
    "tsc-alias": "^1.8.10",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "exports": {
    "./*": "./dist/*.js"
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": ["dist/*.d.ts"]
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": ["./*.ts"]
    }
  }
}
